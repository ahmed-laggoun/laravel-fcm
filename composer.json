{
    "name": "smirltech/laravel-fcm",
    "description": "Laravel Firebase Cloud Messaging.",
    "homepage": "https://github.com/smirltech/laravel-fcm",
    "license": "MIT",
    "authors": [
        {
            "name": "Gentrit Abazi",
            "email": "gentritabazi01@gmail.com"
        },
        {
            "name": "Marien Mupenda",
            "email": "marien@smirlech.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.14.1",
        "ext-http": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "nunomaduro/collision": "^7.6",
        "orchestra/testbench": "^8.0.8",
        "pestphp/pest": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.2.2"
    },
    "autoload": {
        "psr-4": {
            "SmirlTech\\LaravelFcm\\": "src",
            "SmirlTech\\LaravelFcm\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmirlTech\\LaravelFcm\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmirlTech\\LaravelFcm\\Providers\\LaravelFcmServiceProvider"
            ],
            "aliases": {
                "LaravelFcm": "SmirlTech\\LaravelFcm\\Facades\\LaravelFcm"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
